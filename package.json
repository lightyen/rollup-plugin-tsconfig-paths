{
  "name": "rollup-plugin-tsconfig-paths",
  "version": "1.1.2",
  "description": "Rollup plugin for resolving tsconfig paths",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "typings/index.d.ts",
  "author": "lightyen",
  "repository": {
    "url": "https://github.com/lightyen/rollup-plugin-tsconfig-paths"
  },
  "homepage": "https://github.com/lightyen/rollup-plugin-tsconfig-paths#readme",
  "scripts": {
    "types": "tsc --project tsconfig.json --declaration --emitDeclarationOnly --outDir typings",
    "test": "jest",
    "build": "rollup -c",
    "format": "prettier --config .prettierrc.yml --write {src,tests,typings}/**/*",
    "prepare": "jest && yarn build && yarn types && yarn format",
    "doit": "yarn upgrade-interactive --latest"
  },
  "keywords": [
    "rollup",
    "plugin",
    "typescript",
    "tsconfig",
    "paths"
  ],
  "dependencies": {
    "typescript-paths": "^1.2.3"
  },
  "peerDependencies": {
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-typescript": "7.15.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@types/jest": "27.0.2",
    "@types/node": "^16.11.6",
    "jest": "27.3.1",
    "prettier": "^2.4.1",
    "rollup": "2.58.3",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "typescript": "^4.4.4"
  },
  "files": [
    "dist",
    "typings",
    "LICENSE",
    "README.md"
  ]
}
